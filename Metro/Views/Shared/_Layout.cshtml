                        { 
                        <li style="font-family: 微软雅黑; width: 55px;">@Html.ActionLink("登录", "login", "index")</li>
                        <li style="font-family: 微软雅黑; width: 55px;">@Html.ActionLink("注册", "Regist", "index")</li>
                        }
                        else
                        { 
                        <li style="font-family: 微软雅黑; width: 60px;">@Html.ActionLink("个人空间", "Index", "User")</li>
                        <li style="font-family: 微软雅黑; width: 55px;">@Html.ActionLink("退出", "Exit", "Index")</li>
