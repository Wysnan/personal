                    </div>
                </a>
                <div class="message-box">
                    &nbsp;</div>*@
            </div>
            <!-- end share icons -->
        </div>
        <div style="height: 30px;">
        </div>
        @RenderBody()
        <!-- start content -->
        <div class="content-05">
            &nbsp;</div>
        <!-- end content -->
        <div class="clear">
        </div>
        <div class="footer">
            <!-- use for notes -->
            <div class="box-00">
            </div>
            <div class="aside-01">
                <a href="#" target="_blank">
                    <img src="/Content/MetroImage/aicike_log.png" border="0" height="18" width="105" alt="Windows"
